<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">

    <Label layoutX="180.0" layoutY="59.0" text="Categoria">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>


    <Group layoutX="176.0" layoutY="270.0">

        <Button mnemonicParsing="false" onAction="#salvarCategoria" text="Salvar">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>

        <Hyperlink layoutX="105.0" layoutY="8.0" text="Voltar" onAction="#voltar">
            <font>
                <Font size="24.0"/>
            </font>
        </Hyperlink>
    </Group>

    <TextField layoutX="176.0" layoutY="163.0" promptText="Nome" fx:id="tfNomeCategoria"/>

</AnchorPane>
