        <Hyperlink styleClass="linkMenu" layoutX="1044.0" layoutY="65.0" text="Voltar" onAction="#voltar"/>


        <TextField fx:id="tfPesquisarCategoria" styleClass="tfPesquisa" layoutX="99.0" layoutY="55.0" prefHeight="59.0" prefWidth="277.0"
