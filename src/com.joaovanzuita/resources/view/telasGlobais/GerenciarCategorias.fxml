    </Pane>

    <ListView fx:id="listViewCategorias" layoutX="163.0" layoutY="251.0" prefHeight="382.0" prefWidth="320.0"/>

    <Button styleClass="buttonMain" layoutX="803.0" layoutY="357.0" mnemonicParsing="false" text="Excluir" onAction="#excluirCategoria"/>

    <Button styleClass="buttonMain" layoutX="803.0" layoutY="493.0" mnemonicParsing="false" text="Editar" onAction="#editarCategoria"/>

</AnchorPane>
