<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">

    <Pane layoutX="263.0" layoutY="32.0" prefHeight="248.0" prefWidth="291.0">

        <Label fx:id="labelNomeProduto" layoutX="60.0" layoutY="29.0" />
        <Label fx:id="labelCategoriaProduto" layoutX="78.0" layoutY="75.0" />
        <Label fx:id="labelPrecoDeVendaProduto" layoutX="109.0" layoutY="118.0" />
